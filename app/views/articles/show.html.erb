<h1><%= @article.title %></h1>
<p><%= link_to "Edit article", edit_article_path %></p>
<p><%= link_to "Delete article", article_path(@article),
      method: :delete,
      data: { confirm: "Are you sure?" } %></p>

<p><%= @article.content %></p>
<p>Date of creation: <%= @article.created_at %></p>
<p>Last update: <%= @article.updated_at %></p>
<p><%= link_to "Articles list", articles_path %></p>
